    "CP Fast Input: Weighted Graph": {
        "scope": "java",
        "prefix": "inwgraph",
        "body": [
            "        List<int[]>[] nextWeightedGraph(int nodes, int edges, boolean directed) {",
            "            List<int[]>[] adj = new ArrayList[nodes + 1];",
            "            for (int i = 0; i <= nodes; i++) adj[i] = new ArrayList<>();",
            "            for (int i = 0; i < edges; i++) {",
            "                int u = nextInt();",
            "                int v = nextInt();",
            "                int w = nextInt();",
            "                adj[u].add(new int[]{v, w}); // index 0: dest, index 1: weight",
            "                if (!directed) adj[v].add(new int[]{u, w});",
            "            }",
            "            return adj;",
            "        }",
            "        $0"
        ],
        "description": "Prefix: inwgraph -> Builds a Weighted Adjacency List (1-indexed)"
