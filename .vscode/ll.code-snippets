        "scope": "java",
        "prefix": "llcycle",
        "body": [
            "    // Requires listnode snippet",
            "    static ListNode detectCycle(ListNode head) {",
            "        ListNode slow = head, fast = head;",
            "        while (fast != null && fast.next != null) {",
            "            slow = slow.next;",
            "            fast = fast.next.next;",
            "            if (slow == fast) {",
            "                slow = head;",
            "                while (slow != fast) {",
            "                    slow = slow.next;",
            "                    fast = fast.next;",
            "                }",
            "                return slow; // Returns the node where the cycle begins",
            "            }",
            "        }",
            "        return null; // No cycle",
            "    }",
            "    $0"
        ],
        "description": "Prefix: llcycle -> O(N) time, O(1) space cycle detection and entry point locator"
